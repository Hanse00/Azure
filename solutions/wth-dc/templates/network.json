{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {},
    "resources": [
        {
            "comments": "---------- NETWORK WATCHER ----------",
            "type": "Microsoft.Network/networkWatchers",
            "apiVersion": "2020-04-01",
            "name": "[concat('nw-d-', resourceGroup().location)]",
            "location": "[resourceGroup().location]",
            "properties": {}
        },
        {
            "type": "Microsoft.Network/networkSecurityGroups",
            "apiVersion": "2020-05-01",
            "name": "[concat('nsg-shared-d-', resourceGroup().location)]",
            "location": "[resourceGroup().location]",
            "properties": {
                "securityRules": []
            }
        },
        {
            "comments": "---------- VIRTUAL NETWORK ----------",
            "name": "[concat('vnet-d-', resourceGroup().location)]",
            "type": "Microsoft.Network/virtualNetworks",
            "apiVersion": "2018-08-01",
            "location": "[resourceGroup().location]",
            "tags": {},
            "dependsOn": [
                "[concat('nw-d-', resourceGroup().location)]"
            ],
            "properties": {
                "addressSpace": {
                    "addressPrefixes": [
                        "10.0.0.0/21"
                    ]
                },
                "dhcpOptions": {
                    "dnsServers": [
                        "10.0.0.4",
                        "168.63.129.16"
                    ]
                },
                "subnets": [
                    {
                        "name": "[concat('snet-shared-d-', resourceGroup().location)]",
                        "properties": {
                            "addressPrefix": "10.0.0.0/24",
                            "delegations": [],
                            "privateEndpointNetworkPolicies": "Enabled",
                            "privateLinkServiceNetworkPolicies": "Enabled"
                        }
                    }
                ],
                "virtualNetworkPeerings": [
                ],
                "enableDdosProtection": false,
                "enableVmProtection": false
            }
        }
    ]
}